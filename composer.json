{
    "name": "eikoninaru/flarum-ext-ukrainian",
    "type": "flarum-extension",
    "description": "Ukrainian language pack for Flarum.",
    "homepage": "https://github.com/eikoninaru/flarum-ext-ukrainian",
    "keywords": ["locale"],
    "license": "MIT",
    "authors": [
        {
            "name": "sizonov_stas",
            "email": "root@linuxmint.com.ua",
            "homepage": "https://linuxmint.com.ua/"
        },
	{
     	    "name": "Igor Kiselyuk",
     	    "homepage": "http://netishin.com.ua/"
    	}
    ],
    "support": {
        "issues": "https://github.com/eikoninaru/flarum-ext-ukrainian/issues",
        "forum": "https://linuxmint.com.ua/"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.13"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "flarum-extension": {
            "title": "Ukrainian",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#fff",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "uk",
            "title": "Українська"
        }
    }
}
